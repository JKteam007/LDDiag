<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
  </configSections>
  <log4net>
    <appender name="Native" type="log4net.Appender.RollingFileAppender">
      <file value="lddiag.native.log" />
      <appendToFile value="true" />
      <rollingStyle value="Composite" />
      <datePattern value=".yyyyMMdd" />
      <maxSizeRollBackups value="10" />
      <maximumFileSize value="5MB" />
      <staticLogFileName value="true" />
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <layout type="log4net.Layout.Pattern Layout">
        <conversionPattern value="%date [%thread] %-6level %logger - %message %exception%newline" />
      </layout>
    </appender>

    <appender name="Managed" type="log4net.Appender.RollingFileAppender">
      <file value="lddiag.managed.log" />
      <appendToFile value="true" />
      <rollingStyle value="Composite" />
      <datePattern value=".yyyyMMdd" />
      <maxSizeRollBackups value="10" />
      <maximumFileSize value="5MB" />
      <staticLogFileName value="true" />
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <layout type="log4net.Layout.Pattern Layout">
        <conversionPattern value="%date [%thread] %-6level %logger - %message %exception%newline" />
      </layout>
    </appender>


    <logger name="Native">
      <level value="ALL" />
      <appender-ref ref="Native" />
    </logger>

    <logger name="Managed">
      <level value="ALL" />
      <appender-ref ref="Managed" />
    </logger>

    <logger name="Native.DatamartBuilder" additivity="false">
      <level value="ALL" />
      <appender-ref ref="Native" />
    </logger>

    <logger name="Managed.DatamartBuilder" additivity="false">
      <level value="ALL" />
      <appender-ref ref="Managed" />
    </logger>
   
    <root>
      <level value="OFF" />
      <appender-ref ref="" />
    </root>


    
  </log4net>
</configuration>